{
    "name": "wild-siena/google-maps-bundle",
    "description": "Symfony google maps bundle. Create a Google map object in php and render it with twig function.",
    "type": "symfony-bundle",
    "keywords": ["symfony-ux"],
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "WildSiena\\GoogleMapsBundle\\": "src/"
        }
    },
    "require": {
        "php": ">=8.1",
        "symfony/http-kernel": "^6.4 | ^7.1",
        "symfony/stimulus-bundle": "^2.18",
        "phpdocumentor/reflection-docblock": "^5.4",
        "phpstan/phpdoc-parser": "^1.29",
        "symfony/property-access": "^6.4 | ^7.1",
        "symfony/property-info": "^6.4 | ^7.1",
        "symfony/serializer": "^6.4 | ^7.1",
        "symfony/asset-mapper": "^6.4 | ^7.1"
    },
    "require-dev": {
        "phpunit/phpunit": "^10",
        "phpstan/phpstan": "^1.11"
    },
    "config": {
    },
    "scripts": {
        "test-scripts": {
            "phpstan": "vendor/bin/phpstan analyse -c phpstan.neon",
            "phpunit": "vendor/bin/phpunit --testdox tests/"
        }
    }
}
